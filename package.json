{
  "name": "react-pokemons",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.6",
    "formik": "^1.3.1",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-bootstrap": "^0.32.4",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.6.0",
    "react-loadable": "^5.5.0",
    "react-loading": "^2.0.3",
    "react-scripts": "2.1.1",
    "react-transition-group": "^2.5.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-sass": "npm run node-sass-chockidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "moxios": "^0.4.0",
    "node-sass": "^7.0.0",
    "node-sass-chokidar": "^1.3.4",
    "prettier": "^1.15.1"
  }
